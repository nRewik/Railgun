{
  "name": "react-railgun",
  "version": "1.0.0",
  "description": "an elegant universal react-redux starter kit",
  "main": "source/server.js",
  "scripts": {
    "clean": "rimraf build",
    "build-babel": "babel source -d build --copy-files",
    "build-webpack": "webpack --config webpack.config.prod.js",
    "build-webpack-dev": "webpack --config webpack.config.dev.js",
    "build": "npm run clean && npm run build-babel && npm run build-webpack",
    "build-dev": "npm run clean && npm run build-babel && npm run build-webpack-dev",
    "server": "cross-env NODE_PATH=build NODE_ENV=production node build/server/main.js",
    "server-dev": "cross-env NODE_PATH=build NODE_ENV=development node build/server/main.js",
    "start": "npm run build && npm run server",
    "start-dev": "npm run build-dev && npm run server-dev",
    "lint": "eslint --cache .",
    "test": "echo \"test command is not implemented.\"",
    "check": "npm run lint && npm run test && npm outdated --depth=0",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nRewik/react-railgun.git"
  },
  "keywords": [
    "boilerplate",
    "starter",
    "universal",
    "react",
    "redux",
    "hot",
    "reload",
    "hmr",
    "live",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Nutchaphon Rewik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nRewik/react-railgun/issues"
  },
  "homepage": "https://github.com/nRewik/react-railgun#readme",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chokidar": "^1.4.3",
    "cp": "^0.2.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^1.1.1",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "mkdirp": "^0.5.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "updtr": "^0.1.10",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redux": "^3.5.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "webpack-isomorphic-tools": "^2.2.48"
  }
}
