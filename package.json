{
  "name": "railgun",
  "version": "1.0.0",
  "description": "an elegant universal react-redux starter kit",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build-babel": "babel source -d build",
    "build-webpack": "webpack --config webpack.config.production.js",
    "build": "npm run build-babel && npm run build-webpack",
    "lint": "eslint --cache .",
    "test": "echo \"test command is not implemented.\"",
    "check": "npm run lint && npm run test && npm outdated --depth=0",
    "update": "updtr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nRewik/Railgun.git"
  },
  "keywords": [
    "boilerplate",
    "starter",
    "universal",
    "react",
    "redux",
    "hot",
    "reload",
    "hmr",
    "live",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Nutchaphon Rewik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nRewik/Railgun/issues"
  },
  "homepage": "https://github.com/nRewik/Railgun#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^1.1.1",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "updtr": "^0.1.10",
    "webpack": "^1.12.14"
  }
}
